{
  "expo": {
    "name": "WaxMate",
    "slug": "waxmate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jpsignum.waxmate"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jpsignum.waxmate"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "a9297841-fe11-4acc-b1e7-3dd2b9a8c037"
      }
    },
    "owner": "jp_signum",
    "updates": {
      "url": "https://u.expo.dev/a9297841-fe11-4acc-b1e7-3dd2b9a8c037"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
